{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "start": "env-cmd -f dist/config/envoirnment/production.env node dist/server.js",
    "build": "tsc",
    "deploy": "git add . && git commit -m Heroku && git push heroku master",
    "create": "npm run typeorm migration:create -n post",
    "db:migrate": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:generate src/migrations/inital-schema -d src/config/database/index.ts",
    "db:migrate:run": "NODE_ENV=production node --require ts-node/register ./node_modules/typeorm/cli.js migration:run -d src/config/database/index.ts",
    "db:drop": "NODE_ENV=development node --require ts-node/register ./node_modules/typeorm/cli.js schema:drop -d src/config/database/index.ts",
    "db:migrate:revert": "NODE_ENV=development node --require ts-node/register ./node_modules/typeorm/cli.js migration:revert -d src/config/database/index.ts",
    "dev": "env-cmd -f src/config/envoirnment/development.env nodemon src/server.ts",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "test": "NODE_ENV=test mocha -r ts-node/register --exit --timeout 80000 --recursive src/test"
  },
  "dependencies": {
    "@faker-js/faker": "^7.3.0",
    "@sendgrid/mail": "^7.7.0",
    "@types/cors": "^2.8.12",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.2",
    "cloudinary": "^1.28.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "env-cmd": "^10.1.0",
    "express": "^4.17.2",
    "express-rate-limit": "^5.5.1",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.1.4",
    "javascript-time-ago": "^2.3.13",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.34",
    "multer": "^1.4.4",
    "pg": "^8.7.3",
    "slugify": "^1.6.5",
    "socket.io": "^4.5.1",
    "typedi": "^0.10.0",
    "typeorm": "0.3.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.0",
    "@types/express": "^4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.0.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
